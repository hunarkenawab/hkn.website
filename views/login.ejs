<%- include("partials/header") %>

<div class="container min-ht-container">
  <div class="row mt-5 justify-content-center p-4">
    <div class="col-md-6 col-sm-12 p-0">
      <div class="card b-0 form-card-shadow animate ">
        <div class="card-header  member-card-header"></div>
    <div class="card member-signup ">
      
        <div class="card-body px-5">
          <form class="form-style" action="/login" method="post" enctype="multipart/form-data">
        <h2 class="form-h2" >Login <img src="/img/logo.png" class="form-logo"/></h2>

        <div class="form-group signup-form-group">
          <input
            type="email"
            class="form-control"
            name="username"
            placeholder=" Enter Email Address"
            required="required"
          />
        </div>

        <div class="form-group signup-form-group">
          <input
            type="password"
            class="form-control"
            name="password"
            placeholder="Enter Password"
            required="required"
          />
        </div>

        <div class="form-group signup-form-group">
          <button type="submit" class="btn btn-warning btn-lg signup-btn">
            Login
          </button>
        </div>
      </form>
        </div>
      
    </div>
    
      </div>
      <div class="text-center login-text">
        Don't have an account? <a href="/signup">Signup here</a>
      </div>
    </div>
  </div>
</div>
<%- include("partials/footer") %>
